{
  "tasks": {
    "start": "deno run -A app.ts",
    "db-sync":"deno run -A npm:prisma@latest db push --schema=./Prisma/schema.prisma",
    "db-view":"deno run -A npm:prisma studio --schema=./Prisma/schema.prisma",
    "db-gen": "deno run -A npm:prisma@latest generate --schema=./Prisma/schema.prisma"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/dotenv": "jsr:@std/dotenv@0.224.0",
    "djwt": "https://deno.land/x/djwt@v3.0.1/mod.ts", 
    "express": "npm:express@^5.1.0",

    "simple-git": "npm:simple-git@^3.27.0",
    "zod": "npm:zod@^3.25.28",
    "prisma": "npm:prisma@^6.8.2",
    "@prisma/adapter-better-sqlite3": "npm:@prisma/adapter-better-sqlite3@^6.8.2",
    "better-sqlite3": "npm:better-sqlite3"

  },
  "nodeModulesDir": "auto"
}